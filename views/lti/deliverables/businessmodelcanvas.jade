extends ../deliverableContainer

block variables
  - var deliverable = 'businessmodelcanvas'
block deliverable
  div.deliverable-info
    i.fa.fa-th.deliverable-icon
    h1.deliverable-title= getDeliverable('businessmodelcanvas').title

    p.deliverable-description!= getDeliverable('businessmodelcanvas').description

    div.activities-data
      h5.activities-data-title Activities:
      p.activities-data-required
        span.solved-activity-count !{solvedActivitiesCount}
        | /
        span.total-activity-count !{activitiesTotalCount}

  include ../subDeliverables/businessmodelcanvasdefault.jade
